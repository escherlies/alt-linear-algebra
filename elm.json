{
  "type": "package",
  "name": "justgook/elm-alt-math",
  "summary": "A linear algebra library for fast vector and matrix math",
  "license": "BSD-3-Clause",
  "version": "1.0.0",
  "exposed-modules": [
    "AltMath.Vector2",
    "AltMath.Vector3",
    "AltMath.Vector4",
    "AltMath.Matrix4",
    "AltMath.Alternative.Record.Vector2",
    "AltMath.Alternative.Record.Vector3",
    "AltMath.Alternative.Record.Vector4",
    "AltMath.Alternative.Record.Matrix4",
    "AltMath.Alternative.Tuple.Vector2",
    "AltMath.Alternative.Tuple.Vector3",
    "AltMath.Alternative.Tuple.Vector4",
    "AltMath.Alternative.Tuple.Matrix4",
    "AltMath.Alternative.ADT.Vector2",
    "AltMath.Alternative.ADT.Vector3",
    "AltMath.Alternative.ADT.Vector4",
    "AltMath.Alternative.ADT.Matrix4"
  ],
  "elm-version": "0.19.0 <= v < 0.20.0",
  "dependencies": {
    "elm/core": "1.0.0 <= v < 2.0.0",
    "elm-explorations/test": "1.2.2 <= v < 2.0.0"
  },
  "test-dependencies": {

    "elm-explorations/linear-algebra": "1.0.3 <= v < 2.0.0"
  }
}
